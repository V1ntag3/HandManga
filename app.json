{
  "expo": {
    "name": "HandManga",
    "slug": "HandManga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#293330"
    },
    "ios": {
      "bundleIdentifier": "com.alencar.bundle.handmanga",
      "supportsTablet": true,
      "infoPlist": {
        "NSFileProtectionCompleteUntilFirstUserAuthentication": true
      }
    },
    "android": {
      "package": "com.alencar.handmanga",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive-icon.png",
        "backgroundColor": "#293330"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/OceanRush.otf"]
        }
      ]
    ]
  }
}